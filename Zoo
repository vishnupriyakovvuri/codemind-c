#include<stdio.h>
#include<math.h>
int main()
{
    char s[100];
    scanf("%s",s);
    int i,k=0,l=0;
    for(i=0;s[i]!='\0';i++)
    {
        if(s[i]=='z')
        {
            k+=1;
        }
        if(s[i]=='o')
        {
            l+=1;
        }
    }
    if((2*k)==l)
    {
        printf("Yes");
    }
    else
    {
        printf("No");
    }
}
